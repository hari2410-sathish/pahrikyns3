generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Admin {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  name     String?
  role     String  @default("admin")
}

model OtpStore {
  id        Int      @id @default(autoincrement())
  email     String
  otp       String
  expiresAt DateTime
}
